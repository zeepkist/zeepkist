    "@semantic-release/github": "9.2.6",
    "@semantic-release/release-notes-generator": "12.1.0",
    "@tsconfig/cypress": "1.0.2",
    "@tsconfig/node20": "20.1.4",
    "@types/color-hash": "1.0.5",
    "@types/node": "20.11.30",
    "@types/three": "0.162.0",
    "@vitejs/plugin-vue": "5.0.4",
    "@vue/eslint-config-prettier": "7.1.0",

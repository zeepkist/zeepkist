    "less": "4.2.0",
    "npm-run-all2": "6.1.2",
    "prettier": "2.8.8",
    "semantic-release": "23.0.6",
    "sitemap": "7.1.1",
    "start-server-and-test": "2.0.3",
    "stream-to-array": "2.3.0",
    "typescript": "5.4.3",
    "vite": "5.2.6",
    "vue-tsc": "1.6.5"
